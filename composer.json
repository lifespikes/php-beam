{
    "name": "lifespikes/php-beam",
    "description": "Opinionated PHP bootstrap package for LifeSpikes projects",
    "autoload": {
        "psr-4": {
            "LifeSpikes\\PhpBeam\\": "src"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "LifeSpikes\\PhpBeam\\PhpBeamServiceProvider"
            ]
        }
    },
    "require": {
        "inertiajs/inertia-laravel": "^v0.6",
        "lifespikes/laravel-bare": "^0.8.95",
        "spatie/laravel-data": "^3.7",
        "spatie/typescript-transformer": "^2.1",
        "tightenco/ziggy": "^1.5"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "webmozart/assert": "^1.11"
    }
}
